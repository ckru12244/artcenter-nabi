body, h1, h2, h3, h4, h5, h6, p, ul, ol, table, dl, dd {
	margin: 0;
	padding: 0;
}
h1 {
	font-size: 16px;
}
a {
	text-decoration: none;
	color: #000;
}
ul, ol {
	list-style: none;
}
img {
	max-width: 100%;
	vertical-align: top;
	border: none;
}
body {
	font-family: "Open Sans", "Noto Sans KR", sans-serif;
}
.skip_nav a{
	position: fixed;
	left: 0;
	top: -9999px;
	width: 100%;
	height: 40px;
	line-height: 40px;
	text-align: center;
	font-size: 0.875em;
	color: #fff;
	background-color: #000;
	z-index: 10000;
}
.skip_nav a:focus{
	top:0;
}
#start{
	position: relative;
}
header{
	display: flex;
	width: 1250px;
	height: 120px;
	margin: 0 auto;
	justify-content: space-between;
	align-items: center;
}
#start .lnb {
	display: flex;
	flex-direction: column;	/* 세로 배치 */
	align-items: center;	/* 가로 중앙 배치 */
	position: absolute;
	right: 0;
	top: 0;
	z-index: 10;
	width: 100px;
	height: 100%;
	background-color: #fff;
}
#start .lnb .close {
	margin-top: 45px;
	width: 40px;
	height: 40px;
	line-height: 40px;
	text-align: center;
}
#start .lnb .close i {
	font-size: 1.250em;
	color: #000;
	transition: 0.4s;
}
#start .lnb .close a.active i {
	color: #ff330e;
	transform: rotate(45deg);
}
 #start .lnb .search {
	margin-top: 35px;
	width: 40px;
	height: 140px;
}
#start .lnb .search a {
	display: block;
	margin: 0 auto;
	width: 18px;
	height: 18px;
	font-size: 1.125em;
	color: #000;
}
#start .lnb .search img {
	margin-top: 15px;
}
nav > ul{
	display: flex;
}
nav > ul > li{
	position: relative;
}
nav > ul > li > a{
	display: block;
	padding: 0 45px;
	font-size: 16px;
	line-height: 60px;
	font-weight: 600;
}
nav > ul > li:hover > a,
nav > ul > li:focus > a{
	color: #ff330e;
}
nav .sub{
	display: none;
	position: absolute;
	left: 0;
	top: 50px;
	z-index: 999;
}
nav li:hover .sub{
	display: block;
}
nav .sub a:hover{
	background-color: #ff330e;
}
nav .sub a{
	display: block;
	width: 150px;
	height: 26px;
	padding-left: 10px;
	padding-top: 5px;
	margin-top: 5px;
	font-size: 14px;
	color: #fff;
	background-color: #999;
}
#start .slider{
	position: relative;	
	height: 600px;
}
#start .slider .gallery li{
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	opacity: 1;
	transition: opacity 0.3s;
}
#start .slider .gallery li img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
#start .slider .gallery .author{
	position: absolute;
	left: 50%;
	bottom: 30px;
	font-size: 13px;
	font-style: italic;
	color: #fff;
	transform: translateX(-600px);
}
#start .slider .content{
	position: relative;
	margin: 0 auto;
	width: 1250px;
	height: 600px;
}
#start .slider .content .info{
	position: absolute;
	left: 50px;
	top: 50%;
	width: 500px;
	/* z-index: 15; */
	transform: translateY(-50%);
}
#start .slider .content p{
	color: #ff330e;
}
#start .slider .content a{
	display: flex;
	flex-direction: column;
	gap: 5px;
	margin-top: 10px;
}
#start .slider .content a strong{
	display: inline-block;
	padding: 7px 10px;
	font-size: 50px;
	font-weight: 600;
	color: #fff;
	background-color: #ff330e;
}
#start .slider .content .control{
	position: absolute;
	right: 30px;
	bottom: 30px;
}
#start .slider .content .control ul {
	display: flex;
	gap: 8px;
}
#start .slider .content .control a{
	display: block;
	width: 12px;
	height: 12px;
	text-indent: -999px;
	border-radius: 50%;
	background-color: rgba(245,245,245,0.5);
}
#start .slider .content .control li.active a{
	background-color: #ff330e;
}
h3{
	font-size: 17px;
	padding-bottom: 12px;
}
section .desc strong{
	font-size: 16px;
}
section .desc span{
	font-size: 14px;
}
/* section1 */
#section1{
	display: flex;
	gap: 70px;
	margin: 0 auto;
	padding: 80px 0 50px;
	width: 1250px;
}
#section1 .title{
	width: 30%;
}
#section .title p{
	white-space: nowrap;
}
#section1 .title .label{
	display: flex;
	justify-content: space-between;
	padding-top: 27px;
}
#section1 .title .label a{
	display: inline-block;
	color: #fff;
	height: 100%;
	font-size: 10px;
	line-height: 25px;
	padding: 0 6px;
	background-color: #000;
}
#section1 .desc{
	width: calc(70% - 70px);
	line-height: 1.8;
	font-size: 0.875em;
}
/* section2 */
#section2{ /**/
	display: flex;
	gap: 20px;
	width: 1250px;
	margin: 0 auto;
	padding: 60px 0;
}
#section2 .content a{
	display: block;
}
#section2 .content .desc strong{
	display: block;
	padding-bottom: 3px;
}
#section2 .content .desc span{
	display: block;
}
#section2 .academy{
	width: 314px;
}
#section2 .academy .content .img_box{
	height: 274px;
}
#section2 .academy .content .img_box img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
#section2 .academy .content .desc{
	padding: 10px 4px;
}
#section2 .upcoming_project{
	width: 533px;
}
#section2 .upcoming_project .content img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
#section2 .upcoming_project .content .desc{
	padding: 10px 4px;
}
#section2 .archive{
	width: 374px;
}
#section2 .archive .content img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
#section2 .archive .content .desc{
	padding: 10px 4px;
}
#section3{
	padding: 40px 0;
	overflow: hidden;
}
#section3 .inner {
	margin: 0 auto;
	width: 1250px;
}
#section3 .content{
	position: relative;
	width: 100vw;
	height: 275px;
}
#section3 .content > img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
#section3 .desc{
	position: absolute;
	left: 150px;
	top: 50%;
	padding: 12px 20px 20px 25px;
	background-color: #fff;
	transform: translateY(-50%);
}
#section3 .desc strong{
	display: block;
	padding: 10px 0;
	font-size: 24px;
}
#section3 .desc p{
	font-size: 15px;
	font-weight: 500;
}
#section3 .desc span{
	position: absolute;
	right: 20px;
	top: 25px;
}
#section4{
	display: flex;
	gap: 27px;
	width: 1250px;
	height: 100%;
	margin: 0 auto;
	padding: 100px 0;
}
#section4 .whats_on .content{
	width: 478px;
}
#section4 .whats_on .content a img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
#section4 .notice .content{
	padding: 20px 20px;
	background-color: #f0f0f0;
}
#section4 .notice .content .hiring_title > span{
	display: block;
	font-size: 15px;
	font-weight: bold;
	color: #a09d96;
}
#section4 .notice .content .hiring_title a{
	position: relative;
}
#section4 .notice .content .hiring_title a > strong{
	display: block;
	color: #313131;
	font-size: 15px;
	font-weight: 500;
	padding-bottom: 12px;
}
#section4 .notice .content .hiring_title a > span{
	font-size: 14px;
	font-weight: 300;
	color: #a8a8a8;
}
#section4 .notice .content .hiring_title a > p{
	font-size: 14px;
	color: #666666;
	padding-bottom: 28px;
}
#section4 .notice .content .hiring_title a > p::after{
	content: "";
	position: absolute;
	left: 0;
	bottom: 0px;
	width: 390px;
	height: 1px;
	background-color: #ccc;
}
#section4 .notice .content .hiring_list{
	display: flex;
	padding-top: 22px;
}
#section4 .notice .content .hiring_list .hiring_list_title a{
	font-size: 14px;
	color: #838383;
}
#section4 .notice .content .hiring_list .hiring_list_title span{
	font-size: 14px;
	color: #838383;
}
#section4 .notice .content .hiring_list .hiring_list_date li{
	font-size: 14px;
	color: #838383;
}
#section4 .nabi_studies{
	width: 227px;
}
#section4 .nabi_studies .content{
	max-height: 270px;
	overflow: hidden;	
}
	#section4 .nabi_studies .content img{
	display: block;
	width: 100%;
	height: 100%;
	object-fit: cover;
}
footer{
	display: flex;
	background-color: #212121;
}
footer .footer_wrap{
	display: flex;
	width: 1250px;
	margin: 0 auto;
	gap: 130px;
	padding: 150px 90px 50px 0;
	box-sizing: border-box;
}
footer .footer_wrap .footer_left .address{
	padding-bottom: 75px;
}
footer .footer_wrap .footer_left .address strong{
	font-size: 17px;
	color: #e7e7e7;
}
footer .footer_wrap .footer_left .address p{
	padding-top: 40px;
	font-size: 13px;
	color: #9aa1a8;
}
footer .footer_wrap .footer_left .sns{
	position: relative;
}
footer .footer_wrap .footer_left .sns ul{
	display: flex;
	gap: 18px;
	justify-content: end;
}
footer .footer_wrap .footer_left .sns::before{
	content: "";
	position: absolute;
	left: 0;
	top: 50%;
	width: calc(100% - 170px);
	height: 1px;
	background-color: #666666;
}
footer .footer_wrap .footer_left .sns ul li a{
	font-size: 27px;
	color: #adb5bd;
}
footer .footer_wrap .footer_left .privacy{
	padding-top: 75px;
	gap: 8px;
}
footer .footer_wrap .footer_left .privacy ul{
	display: flex;
	flex-wrap: wrap;
}
footer .footer_wrap .footer_left .privacy .top a{
	position: relative;
	display: block;
	padding: 0 10px;
	font-size: 13px;
	color: #9aa1a8;
}
footer .footer_wrap .footer_left .privacy .top li:first-child a{
	padding-left:0;
}
footer .footer_wrap .footer_left .privacy .top a::after{
	content: "";
	position: absolute;
	right: 0;
	top: 3px;
	width: 1px;
	height: 16px;
	background-color: #9aa1a8;
}
footer .footer_wrap .footer_left .privacy .top li:last-child a::after{
	content: "";
	position: absolute;
	display: none;
}
footer .footer_wrap .footer_left .privacy .bottom li{
	position: relative;
	padding: 0 10px;
	font-size: 13px;
	color: #656565;
}
footer .footer_wrap .footer_left .privacy .bottom li:first-child{
	padding-left: 0;
}
footer .footer_wrap .footer_left .privacy .bottom li::after{
	content: "";
	position: absolute;
	right: 0;
	top: 3px;
	width: 1px;
	height: 16px;
	background-color: #9aa1a8;
}
footer .footer_wrap .footer_left .privacy .bottom li:last-child::after{
	content: "";
	position: absolute;
	display: none;
}
footer .footer_wrap .footer_right{
	display: flex;
	margin-top: 140px;
	gap: 97px;
}
footer .footer_wrap .footer_right span{
	position: relative;
	font-size: 20px;
	font-weight: 600;
	color: #9aa1a8;
}
footer .footer_wrap .footer_right span::after{
	content: "";
	position: absolute;
	left: 0;
	bottom: -24px;
	width: 215px;
	height: 1px;
	background-color: #adb5bd;
}
footer .footer_wrap .footer_right p{
	padding-top: 35px;
	font-size: 13px;
	color: #9aa1a8;
}